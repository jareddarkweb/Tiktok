-d '{
    "advertiser_id": "ADVERTISER_ID",
    "budget_mode": "BUDGET_MODE_DAY",
    "objective_type": "REACH",
    "budget": "99.99",
    "campaign_name": "Hello Campaign"
}' \
https://sandbox-ads.tiktok.com/open_api/v1.2/campaign/create/